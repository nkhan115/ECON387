import math

x = 2.
print(type(x))
x = 2.0
print(type(x))
x = "2.0"
print(type(x))
x = 2 + 0j
print(type(x))
x = "2 + 0j"
print(type(x))
x = math.pi
print(type(x))

s = "Python has a nice syntax"
print(s)
length = len(s)
print(length)
print(s[18:24])
print(s.find('a'))
print(s.replace("a", "A"))
b = " But, Stata's is counterintuitive."
s = s + "." + b
print(s)
